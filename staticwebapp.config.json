{
  "navigationFallback": {
    "rewrite": "/index.html"
  },
  "routes": [
    {
      "route": "/login/microsoft",
      "rewrite": "/.auth/login/aad"
    },
    {
      "route": "/login/google",
      "rewrite": "/.auth/login/google"
    },
    {
      "route": "/login/github",
      "rewrite": "/.auth/login/github"
    },
    {
      "route": "/logout",
      "rewrite": "/.auth/logout"
    },
    {
      "route": "/api/*",
      "methods": [
        "GET",
        "POST",
        "PUT",
        "DELETE"
      ],
      "allowedRoles": [
        "anonymous",
        "authenticated"
      ]
    }
  ]
}