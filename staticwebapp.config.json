{
  "navigationFallback": {
    "rewrite": "/index.html"
  },
  "auth": {
    "identityProviders": {
      "google": {
        "registration": {
          "clientIdSettingName": "GOOGLE_CLIENT_ID",
          "clientSecretSettingName": "GOOGLE_CLIENT_SECRET"
        }
      }
    }
  },
  "routes": [
    {
      "route": "/login/microsoft",
      "rewrite": "/.auth/login/aad?prompt=select_account"
    },
    {
      "route": "/login/google",
      "rewrite": "/.auth/login/google"
    },
    {
      "route": "/logout",
      "rewrite": "/.auth/logout"
    },
    {
      "route": "/api/*",
      "methods": [
        "GET",
        "POST",
        "PUT",
        "DELETE"
      ],
      "allowedRoles": [
        "anonymous",
        "authenticated"
      ]
    }
  ]
}